{
  "npmScope": "matheo",
  "implicitDependencies": {
    "angular.json": "*",
    "package.json": {
      "dependencies": "*",
      "devDependencies": "*"
    },
    "tslint.json": "*",
    "nx.json": "*",
    "tsconfig.base.json": "*"
  },
  "cli": {
    "warnings": {
      "versionMismatch": false
    },
    "defaultCollection": "@nrwl/angular"
  },
  "defaultProject": "website",
  "projects": {
    "datasource": {
      "tags": [
        "shared"
      ]
    },
    "datepicker": {
      "tags": [
        "shared"
      ]
    },
    "schematics": {
      "tags": [
        "shared"
      ]
    },
    "website": {
      "tags": [
        "app"
      ]
    },
    "website-e2e": {
      "tags": [],
      "implicitDependencies": [
        "website"
      ]
    }
  },
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/workspace/tasks-runners/default",
      "options": {
        "cacheableOperations": [
          "build",
          "lint",
          "test",
          "e2e"
        ]
      }
    }
  },
  "affected": {
    "defaultBase": "master"
  },
  "targetDependencies": {
    "build": [
      {
        "target": "build",
        "projects": "dependencies"
      }
    ]
  },
  "generators": {
    "@nrwl/angular:application": {
      "style": "scss",
      "linter": "tslint",
      "unitTestRunner": "jest"
    },
    "@nrwl/angular:library": {
      "style": "scss",
      "linter": "tslint",
      "unitTestRunner": "jest",
      "strict": true
    },
    "@schematics/angular:component": {
      "style": "scss",
      "changeDetection": "OnPush"
    }
  }
}
