{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "",
  "defaultProject": "website",
  "projects": {
    "website": {
      "root": "apps/website/",
      "sourceRoot": "apps/website/src",
      "projectType": "application",
      "prefix": "web",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/apps/website",
            "index": "apps/website/src/index.html",
            "main": "apps/website/src/main.ts",
            "polyfills": "apps/website/src/polyfills.ts",
            "tsConfig": "apps/website/tsconfig.app.json",
            "assets": [
              "apps/website/src/assets",
              {
                "glob": "**/*",
                "input": "apps/website/src/favicons/",
                "output": ""
              }
            ],
            "styles": ["apps/website/src/styles.scss"],
            "stylePreprocessorOptions": {
              "includePaths": ["apps/website/src/styles"]
            },
            "scripts": [],
            "allowedCommonJsDependencies": ["lodash"],
            "vendorChunk": true,
            "extractLicenses": false,
            "buildOptimizer": false,
            "sourceMap": true,
            "optimization": false,
            "namedChunks": true
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/website/src/environments/environment.ts",
                  "with": "apps/website/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb"
                }
              ]
            }
          },
          "outputs": ["dist/apps/website"],
          "defaultConfiguration": ""
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "website:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "website:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "website:build"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "apps/website/tsconfig.app.json",
              "apps/website/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!apps/website/**"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/website/jest.config.js",
            "passWithNoTests": true
          },
          "outputs": ["coverage/apps/website/"]
        }
      }
    },
    "website-e2e": {
      "root": "apps/website-e2e",
      "projectType": "application",
      "prefix": "",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/website-e2e/cypress.json",
            "tsConfig": "apps/website-e2e/tsconfig.e2e.json",
            "devServerTarget": "website:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "website:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": "apps/website-e2e/tsconfig.e2e.json",
            "exclude": ["**/node_modules/**", "!apps/website-e2e/**/*"]
          }
        }
      }
    },
    "datasource": {
      "root": "libs/datasource",
      "sourceRoot": "libs/datasource/src",
      "projectType": "library",
      "prefix": "mat",
      "architect": {
        "build": {
          "builder": "@nrwl/angular:package",
          "options": {
            "tsConfig": "libs/datasource/tsconfig.lib.json",
            "project": "libs/datasource/ng-package.json",
            "buildableProjectDepsInPackageJsonType": "dependencies"
          },
          "configurations": {
            "production": {
              "tsConfig": "libs/datasource/tsconfig.lib.prod.json"
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "libs/datasource/tsconfig.lib.json",
              "libs/datasource/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!libs/datasource/**/*"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/datasource/jest.config.js",
            "passWithNoTests": true
          },
          "outputs": ["coverage/libs/datasource"]
        }
      }
    },
    "datepicker": {
      "root": "libs/datepicker",
      "sourceRoot": "libs/datepicker/src",
      "projectType": "library",
      "prefix": "mat",
      "architect": {
        "build": {
          "builder": "@nrwl/angular:package",
          "options": {
            "tsConfig": "libs/datepicker/tsconfig.lib.json",
            "project": "libs/datepicker/ng-package.json",
            "buildableProjectDepsInPackageJsonType": "dependencies"
          },
          "configurations": {
            "production": {
              "tsConfig": "libs/datepicker/tsconfig.lib.prod.json"
            }
          }
        },
        "postbuild": {
          "builder": "@nrwl/workspace:run-commands",
          "options": {
            "commands": [
              "node ./node_modules/.bin/scss-bundle -p . --ignoreImports '~@angular/.*' -e libs/datepicker/src/lib/_datepicker-theme.scss -o dist/libs/datepicker/_theming.scss",
              "node ./node_modules/.bin/node-sass --importer tools/node-sass.importer.js --output-style compressed -o dist/libs/datepicker/prebuilt-themes/ libs/datepicker/src/lib/prebuilt-themes/"
            ]
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "libs/datepicker/tsconfig.lib.json",
              "libs/datepicker/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!libs/datepicker/**/*"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/datepicker/jest.config.js",
            "passWithNoTests": true
          },
          "outputs": ["coverage/libs/datepicker"]
        }
      }
    },
    "forms": {
      "projectType": "library",
      "root": "libs/forms",
      "sourceRoot": "libs/forms/src",
      "prefix": "dyn",
      "architect": {
        "build": {
          "builder": "@nrwl/angular:package",
          "options": {
            "tsConfig": "libs/forms/tsconfig.lib.json",
            "project": "libs/forms/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "libs/forms/tsconfig.lib.prod.json"
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "libs/forms/tsconfig.lib.json",
              "libs/forms/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!libs/forms/**/*"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/forms"],
          "options": {
            "jestConfig": "libs/forms/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "schematics": {
      "root": "libs/schematics",
      "sourceRoot": "libs/schematics/src",
      "projectType": "library",
      "prefix": "mat",
      "architect": {
        "build": {
          "builder": "@nrwl/angular:package",
          "options": {
            "tsConfig": "libs/schematics/tsconfig.lib.json",
            "project": "libs/schematics/ng-package.json",
            "buildableProjectDepsInPackageJsonType": "dependencies"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "libs/schematics/tsconfig.lib.json",
              "libs/schematics/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!libs/schematics/**/*"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/schematics/jest.config.js",
            "passWithNoTests": true
          },
          "outputs": ["coverage/libs/schematics"]
        }
      }
    }
  },
  "cli": {
    "warnings": {
      "versionMismatch": false
    },
    "defaultCollection": "@nrwl/angular"
  },
  "schematics": {
    "@nrwl/angular:application": {
      "style": "scss",
      "linter": "tslint",
      "unitTestRunner": "jest"
    },
    "@nrwl/angular:library": {
      "style": "scss",
      "linter": "tslint",
      "unitTestRunner": "jest",
      "strict": true
    },
    "@schematics/angular:component": {
      "style": "scss",
      "changeDetection": "OnPush"
    }
  }
}
